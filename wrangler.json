{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cleverli",
  "compatibility_date": "2025-01-15",
  "compatibility_flags": ["nodejs_compat"],
  "main": "dist/_worker.js",
  "assets": {
    "directory": "./dist"
  },
  "vars": {
    "NOTIFICATION_EMAILS": "sitecteam25@gmail.com,info@cleverli.pro"
  },
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}
